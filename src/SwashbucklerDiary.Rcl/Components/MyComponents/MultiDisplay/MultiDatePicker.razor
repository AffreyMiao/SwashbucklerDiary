@namespace SwashbucklerDiary.Rcl.Components
@inherits DialogComponentBase

<MultiDisplay OnUpdate="UpdateDisplay">
    <MobileContent>
        <PMobileDatePickerExtension MyVisible="Visible"
                                    MyVisibleChanged="InternalVisibleChanged"
                                    Value="Value"
                                    ValueChanged="ValueChanged"
                                    Min="Min??default"
                                    Max="Max??default">
        </PMobileDatePickerExtension>
    </MobileContent>
    <DesktopContent>
        <DatePickerDialog Visible="Visible"
                          VisibleChanged="InternalVisibleChanged"
                          Value="Value"
                          ValueChanged="ValueChanged"
                          Min="Min"
                          Max="Max">
        </DatePickerDialog>
    </DesktopContent>
</MultiDisplay>