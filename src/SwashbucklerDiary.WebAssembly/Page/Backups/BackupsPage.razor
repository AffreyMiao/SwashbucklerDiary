@page "/backups"
@namespace SwashbucklerDiary.WebAssembly.Pages
@inherits BackupsPageBase

<MyAppBar>
    <MButton Icon
             OnClick="NavigateToBack">
        <MIcon>
            mdi-arrow-left
        </MIcon>
    </MButton>

    <MultiTabList @bind-Value="tab"
                  Items="tabNames">
    </MultiTabList>

    <MSpacer></MSpacer>
</MyAppBar>

<SwiperTabItems @bind-Value="tab">
    <SwiperTabItem>
        <LocalBackups></LocalBackups>
    </SwiperTabItem>
    <SwiperTabItem>
        <WebDAVBackups></WebDAVBackups>
    </SwiperTabItem>
</SwiperTabItems>


