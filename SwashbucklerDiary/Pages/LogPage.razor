@page "/log"
@inherits PageComponentBase

<MyAppBar Title="@(I18n.T("About.LogFile"))" OnClick="NavigateToBack">

</MyAppBar>
<ScrollContainer>
    <MList Dense>
        @foreach (var item in LogTextList)
        {
            <MListItem>
                <MListItemContent>
                    @item
                </MListItemContent>
            </MListItem>
        }
    </MList>
</ScrollContainer>

