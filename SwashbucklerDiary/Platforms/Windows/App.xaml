<maui:MauiWinUIApplication
    x:Class="SwashbucklerDiary.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:SwashbucklerDiary.WinUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light">
                    <SolidColorBrush x:Key="WindowCaptionBackground">#f7f8f9</SolidColorBrush>
                    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled">#f7f8f9</SolidColorBrush>
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="WindowCaptionBackground">#121212</SolidColorBrush>
                    <SolidColorBrush x:Key="WindowCaptionBackgroundDisabled">#121212</SolidColorBrush>
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</maui:MauiWinUIApplication>
